{
  "name": "express-torrent-api",
  "version": "1.0.0",
  "description": "A REST APIs wrapper for node.js bittorrent (webtorrent) using express, mongoose in ES7",
  "main": "index.js",
  "author": "Andrew Hart andruwhart@gmail.com>",
  "license": "MIT",
  "private": false,
  "engines": {
    "node": ">=6.9.x",
    "npm": ">=2.15.x"
  },
  "scripts": {
    "prestart": "yarn build:server",
    "precommit": "yarn lint && yarn test",
    "prepush": "yarn lint && yarn test",
    "start": "node dist/server/index.js",
    "dev": "nodemon --exec DEBUG=node* node_modules/babel-cli/bin/babel-node.js server",
    "build": "yarn build:server",
    "build:server": "node_modules/babel-cli/bin/babel.js server --out-dir dist/server",
    "lint": "node_modules/eslint-watch/bin/esw *.js server config --color",
    "lint:watch": "yarn lint -- --watch",
    "test": "cross-env NODE_ENV=test babel-node node_modules/mocha/bin/_mocha -- --ui bdd --reporter spec --colors --compilers js:babel-core/register ./server/**/*.test.js",
    "test:watch": "yarn test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AndrewJHart/express-torrent-api.git"
  },
  "keywords": [
    "express",
    "node",
    "node.js",
    "mongodb",
    "mongoose",
    "starter",
    "es6",
    "es7",
    "mocha",
    "REST",
    "API",
    "bittorrent",
    "torrent"
  ],
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "bluebird": "3.4.6",
    "body-parser": "1.15.2",
    "compression": "1.6.2",
    "cookie-parser": "1.4.3",
    "cors": "2.8.1",
    "debug": "^2.2.0",
    "express": "4.14.0",
    "express-jwt": "5.1.0",
    "express-validation": "1.0.1",
    "express-winston": "2.0.0",
    "faker": "3.1.0",
    "helmet": "3.1.0",
    "http-status": "^0.2.0",
    "joi": "9.2.0",
    "jsonwebtoken": "7.1.9",
    "lodash": "4.17.4",
    "method-override": "^2.3.5",
    "mongoose": "4.6.8",
    "morgan": "1.7.0",
    "webtorrent": "^0.98.20",
    "winston": "2.3.0"
  },
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-core": "6.18.0",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-transform-object-assign": "6.8.0",
    "babel-preset-latest": "^6.16.0",
    "chai": "^3.4.1",
    "commitizen": "^2.8.2",
    "coveralls": "^2.11.6",
    "cross-env": "3.1.4",
    "cz-conventional-changelog": "1.2.0",
    "del": "^2.2.0",
    "eslint": "3.10.2",
    "eslint-config-airbnb-base": "7.1.0",
    "eslint-plugin-import": "1.16.0",
    "eslint-watch": "2.1.14",
    "gulp": "3.9.1",
    "gulp-babel": "6.1.2",
    "gulp-load-plugins": "^1.2.0",
    "gulp-newer": "^1.1.0",
    "gulp-nodemon": "^2.0.6",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-util": "^3.0.7",
    "husky": "^0.13.1",
    "isparta": "4.0.0",
    "mocha": "3.1.2",
    "run-sequence": "^1.1.5",
    "supertest": "2.0.1",
    "supertest-as-promised": "4.0.2",
    "validate-commit-msg": "^2.6.1"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run lint && npm test"
    },
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "babel": {
    "presets": [
      "latest"
    ],
    "plugins": [
      "add-module-exports",
      "transform-object-assign"
    ]
  }
}
